.center{
  text-align: center;
  margin: auto 0;
}
.font{
  font-family: 幼圆;
}
/*横向布局*/
.horizontal{
  display: flex;
  flex-direction: row;
}
.content{
  position: fixed;
  display: flex;
  margin-top: 15rpx;
  background-size: cover;
  height:100vh;
  width: 100%;
  background-image: url("http://pic1.win4000.com/mobile/2019-10-31/5dbaa3f8b8be2.jpg");
  color: #FFF;
  z-index: -1;
}
.img{
  width: 28rpx;
  height: 28rpx;
  margin-right: 16rpx;
}
.wrapper {
  position: fixed;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  font-size: 28rpx;
  height: 60rpx;
  width: 85%;
  margin: 15rpx 50rpx;
  padding: 0 15rpx;
  box-sizing: border-box;
  border: none;
  border-bottom: 1rpx solid #fff;
}

.position{
  font-family: 微软雅黑; 
  color:#FFF;
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.top{
  display: flex;
  justify-content: center;
}
.threehour{
  display: flex;
  flex-direction: row;
}
.oval{
      width: 120rpx;
      height: 232rpx;
      border: 1rpx solid #fff;
      border-radius: 69rpx / 100rpx;
      margin-top: 25rpx;
      margin-right: 20rpx;
}
.swper{
  font-size: 30rpx;
  color: #FFF;
}
.index_img{
  width: 100rpx;
  height: 100rpx;
  display:flex; 
  align-items: center; 
  margin-right:20rpx;
}
.index{
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  padding: 30rpx 0;  
  color: white;
}